        app:layout_constraintLeft_toRightOf="@id/cardsview0"
        app:layout_constraintRight_toLeftOf="@id/cardsview1"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <com.talka.fancybridge.Views.CardView
        android:id="@+id/cardview9"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="1dp"
        android:scaleType="fitXY"
        app:layout_constraintTop_toBottomOf="@id/cardsview6"
        app:layout_constraintLeft_toRightOf="@id/cardsview1"
        app:layout_constraintRight_toLeftOf="@id/cardsview2"
        app:layout_constraintBottom_toBottomOf="parent"/>

